{
  "name": "datamanager",
  "version": "1.0.0",
  "description": "Datamanager to work with react components and flux",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint lib/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && ./node_modules/.bin/karma start --browsers PhantomJS --single-run",
    "test:watch": "karma start --browsers PhantomJS",
    "test:mac": "karma start --single-run --browsers Chrome,Firefox,Safari,Opera"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/datamanager.git"
  },
  "keywords": [
    "react",
    "data",
    "management",
    "flux",
    "immutable.js"
  ],
  "author": "Adrian Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/datamanager/issues"
  },
  "homepage": "https://github.com/djforth/datamanager#readme",
  "devDependencies": {
    "@djforth/angular-jasmine-helpers": "^1.0.7",
    "ajax-es6-module": "1.0.1",
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "isparta": "^4.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-browserify": "^5.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-story-reporter": "^0.3.1",
    "lodash": "^4.3.0",
    "moment": "^2.10.6",
    "moment-strftime": "^0.1.5",
    "phantomjs-prebuilt": "^2.1.3",
    "rewireify": "^0.2.5",
    "sinon": "^1.17.3",
    "watchify": "^3.7.0"
  },
  "peerDependencies": {
    "ajax-es6-module": "1.0.1",
    "lodash": "^4.3.0",
    "moment": "^2.10.6",
    "moment-strftime": "^0.1.5"
  }
}
